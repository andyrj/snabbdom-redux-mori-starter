{
  "name": "snabbdom-redux-mori-starter",
  "version": "0.0.1",
  "description": "starter kit for snabbdom redux mori project, implementing yet another todo app",
  "main": "index.js",
  "scripts": {
    "postinstall": "mkdir -p dist/ && npm run clean",
    "clean": "rm -f dist/*.js && rm -f dist/*.gz && rm -f dist/*.map && cp mori.min.js dist/",
    "build": "npm run clean && better-npm-run dev",
    "build:prod": "npm run clean && better-npm-run prod",
    "build:watch": "npm run clean && better-npm-run dev-watch",
    "lint": "eslint src/; exit 0",
    "lint:fix": "eslint --fix src/; exit 0",
    "start": "npm run build && better-npm-run start-dev",
    "start:prod": "npm run build:prod && better-npm-run start-prod",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover _mocha -- --compilers js:babel-core/register  --require ./test/test_helper.js 'test/**/*.@(js|jsx)' && rm -rf ~/Downloads/coverage && mv ./coverage ~/Downloads/",
    "test:codacy": "babel-node ./node_modules/.bin/isparta cover _mocha --report lcovonly -- --compilers js:babel-core/register  --require ./test/test_helper.js 'test/**/*.@(js|jsx)' && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "test:watch": "npm run test -- --watch",
    "test:nsp": "nsp check"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev-watch": {
      "command": "webpack -w",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "webpack -p",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": " babel-node src/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-prod": {
      "command": "babel-node src/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "Andy Johnson <andyrjohnson82@gmail.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "chokidar": "^1.4.3",
    "http-hash": "^2.0.0",
    "marked": "^0.3.5",
    "mori": "^0.3.2",
    "most": "^0.18.3",
    "node-srp": "0.0.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "rest": "^1.3.1",
    "serve-favicon": "^2.3.0",
    "snabbdom": "^0.4.0",
    "snabbdom-to-html": "^2.1.3",
    "uuid": "^2.0.1",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "ansi-to-html-umd": "^0.4.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "codacy-coverage": "^1.1.3",
    "css-loader": "^0.23.1",
    "deep-freeze-strict": "^1.1.1",
    "eslint": "^2.3.0",
    "eslint-config-google": "^0.4.0",
    "file-loader": "^0.8.5",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "jsdom": "^8.0.4",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "nsp": "^2.2.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "watch": "^0.17.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.8.0"
  }
}
